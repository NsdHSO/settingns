<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.fish.autobackup</string>

    <key>ProgramArguments</key>
    <array>
        <string>/opt/homebrew/bin/fish</string>
        <string>-c</string>
        <string>backup_fish_config</string>
    </array>

    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>23</integer>
        <key>Minute</key>
        <integer>59</integer>
    </dict>

    <key>StandardOutPath</key>
    <string>/Users/davidsamuel.nechifor/.config/fish/hooks/backup.log</string>

    <key>StandardErrorPath</key>
    <string>/Users/davidsamuel.nechifor/.config/fish/hooks/backup.error.log</string>

    <key>RunAtLoad</key>
    <false/>
</dict>
</plist>
